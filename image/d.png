                    $.ajax({
                        url: url,
                        type: "POST",
                        data: {form_key:form_key,sid:id,product_id:product_id},
                        cache: false,
                        async: false,
                        dataType: 'json',
                        success: function(data, status, xhr) {
                            if (data.offer_data.success){
                                updateAllSections(data.offer_data);
                            } else {
                                sellerOfferNotFound();
                            }
                        },
                        error: function (xhr, status, errorThrown) {
                            console.log('Error happens. Try again.');
                            // console.log(errorThrown);
                        },
                        complete: function (data) {
                            fullScreenLoader.stopLoader();
                        },
                        always: function (data){
                            fullScreenLoader.stopLoader();
                        }
                    });
                } else {
                    sellerOfferNotFound();
                }
            });

            function sellerOfferNotFound() {
                $(".price-wrapper").show();
                $(".discount").show();
                $("body").attr('out-of-stock', 'na');
                fullScreenLoader.stopLoader();
            }

            function updateAllSections(offer_data) {
                updateAddToCartAccOfferStock(offer_data);
                updatePriceSection(offer_data);
                updateMinicashSection();
                updateWarrantySectoion(offer_data);
                updateWishListSection();
            }

            function updateAddToCartAccOfferStock(offer_data){
                try {
                    let isSaleable = offer_data.is_saleable;
                    if (isSaleable == false) {
                        $(".notify-stock-wrapper").show();
                        $(".product-addtocart-button").hide();
                        $("body").attr('out-of-stock', 1);
                        $('.stock.unavailable').show();
                        $('.stock.available').hide();
                    } else {
                        $(".notify-stock-wrapper").hide();
                        $(".product-addtocart-button").show();
                        $("body").attr('out-of-stock', 0);
                        $('.stock.unavailable').hide();
                        $('.stock.available').show();
                    }
                } catch (e) {
                }
            }

            function updateWishListSection() {
                try {
                    $(".towishlist-w").hide();
                    $(".action.towishlist").hide();
                } catch (e) {

                }
            }

            function updatePriceSection(offer_data) {
                try {
                    $(".price-wrapper ").show();
                    if (offer_data.hasSpecialPrice == true){
                        $("#product-price-"+product_id+" > span.price").html(offer_data.special_price);
                        $('meta[itemprop=price]').attr('content', offer_data.special_price);
                        $(".old-price-w .discount").show();
                        $(".old-price-w .old-price.was-price .price-wrapper .price").html(offer_data.orgprice);
                        $(".non_dealer_price .old-price-w span.save-label").html(offer_data.special_price_data.save_label);

                    } else {
                        $("#product-price-"+product_id+" > span.price").html(offer_data.orgprice);
                        $('meta[itemprop=price]').attr('content', offer_data.orgprice);
                    }
                } catch (e) {

                }
            }

            function updateMinicashSection() {
                try {
                    if ( (btechVendorId != id) && (id != false) )
                    {
                        $('#installment-component').hide();
                        $('#buy-with-minicash-installment').hide();
                        $('.minicash-campaign-section-info').hide();
                        $('.installment-section-info').hide();
                    }
                } catch (e) {
                }
            }

            function updateWarrantySectoion(offer_data) {
                try {
                    if ( offer_data.warrenty_txt != "" ) {
                        $('.warrenty-txt').html(offer_data.warrenty_txt);
                        $('.number_years_warranty_val').html(offer_data.warrenty_txt);
                        $('.warrenty-label').html(offer_data.warrenty_label);
                        $('.number_years_warranty_label').html(offer_data.warrenty_label);
                    } else {
                        $('.warrenty-w').remove();
                        $('.warrenty-w-l').remove();
                    }
                } catch (e) {

                }
            }
        });
    });
</script>
